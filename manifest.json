{
  "manifest_version": 3,
  "name": "CHROME-STARTER",
  "version": "0.0.1",
  "description": "Chrome Extension Starter, Demo Example",
  "icons": {
    "16": "/icons/icon16.png",
    "32": "/icons/icon32.png",
    "48": "/icons/icon48.png"
  },
  "permissions": [],
  "web_accessible_resources": [
    {
      "resources": ["popup/popup.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["scripts/contentScript.js"]
    }
  ],

  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup/popup.html"
  }
}
